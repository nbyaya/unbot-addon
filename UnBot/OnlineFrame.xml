				<OnLoad>
					OnlineFrame.slider = self;
				</OnLoad>
				<OnValueChanged>
					OnlineFrame.scrollFrame:SetVerticalScroll(self:GetValue());
				</OnValueChanged>
			</Scripts>
          </Slider>
        </Frames>
